   "source": [
    "# Imports\n",
    "from sklearn.svm import SVC"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "807ee0b0",
   "metadata": {},
   "source": [
    "Data Preparation"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "76137b2b",
   "metadata": {},
   "outputs": [],
   "source": [
    "X = [[0, 0], [1, 1]]\n",
    "y = [0, 1]"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "25ff9464",
   "metadata": {},
   "source": [
    "Model Usage"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "70e4e0dc",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "array([1])"
      ]
     },
     "execution_count": 4,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "model = SVC()\n",
    "model.fit(X, y)\n",
    "prediction = model.predict([[2.0, 2.0]])\n",
    "\n",
    "prediction"
   ]
  }
